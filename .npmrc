# Node.js CI/CD and Dependency Management
# This file helps ensure consistent dependency installation across environments

# Package manager configuration
engine-strict=true
audit-level=moderate
fund=false

# Registry and caching configuration
registry=https://registry.npmjs.org/

# Security and peer dependencies
strict-peer-deps=false
legacy-peer-deps=true

# Performance optimizations
prefer-offline=true
progress=false
